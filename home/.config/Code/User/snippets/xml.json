	"Parent": {
		"prefix": "parent",
		"body": [
			"<parent>",
			"\t<groupId>org.springframework.boot</groupId>",
			"\t<artifactId>spring-boot-starter-parent</artifactId>",
			"\t<version>${3.1.1}</version>",
			"\t<relativePath/> <!-- lookup parent from repository -->",
			"</parent>$0"
		],
		"description": "Parent"
	},
