{
    "workbench.colorTheme": "Default Light Modern",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.requirements.JDK11Warning": false,
    "java.semanticHighlighting.enabled": true,
    "remote.SSH.configFile": "/home/dsyer/.ssh/config",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "java.project.importOnFirstTimeStartup": "automatic",
    "maven.executable.preferMavenWrapper": true,
    "java.dependency.packagePresentation": "hierarchical",
    "java.dependency.showMembers": true,
    "editor.renderWhitespace": "all",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "java.refactor.renameFromFileExplorer": "autoApply",
    "go.useLanguageServer": true,
    "redhat.telemetry.enabled": true,
    // "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m",
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "FSharp.suggestGitignore": false,
    "editor.detectIndentation": false,
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "boot-java.rewrite.reconcile": true,
    "npm.keybindingsChangedWarningShown": true,
    "terminal.integrated.defaultProfile.linux": "bash",
    "[java]": {
        "editor.suggest.snippetsPreventQuickSuggestions": false
    },
    "window.zoomLevel": 1,
    "java.server.launchMode": "Standard",
    "editor.stickyScroll.enabled": true,
    "[python]": {
        "editor.formatOnType": true
    },
	"editor.insertSpaces": false,
	"editor.inlineSuggest.enabled": true,
	"github.copilot.enable": {
		"*": true,
		"plaintext": true,
		"markdown": true,
		"scminput": false
	},
	"vs-kubernetes": {
		"vs-kubernetes.ignore-recommendations": true
	},
	"java.import.gradle.enabled": false,
	"java.maven.downloadSources": true
}